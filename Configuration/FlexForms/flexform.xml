<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>settings</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.pageslive>

                        <TCEforms>
                            <label>Selected Pages</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <minitems>1</minitems>
                                <maxitems>1000</maxitems>
                                <size>3</size>
                            </config>
                        </TCEforms>
                    </settings.pageslive>
                    <settings.tableslive>
                        <TCEforms>
                            <label>Table where the contents live</label>
                            <config>
                                <type>select</type>
                                <itemsProcFunc>Pits\McGooglesitemap\Utility\Main->populateItems</itemsProcFunc>
                                <renderType>selectSingle</renderType>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </settings.tableslive>
                    <settings.lastmodif>
                        <TCEforms>
                            <label>Last Modification Style</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items type="array">
                                    <numIndex index="0">
                                        <numIndex index="0">Date</numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">Date &amp; Time</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.lastmodif>
                    <settings.pagescontent>
                        <TCEforms>
                            <label>Page where we can see the contents</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.pagescontent>
                    <settings.url>
                        <TCEforms>
                            <label>
                                LLL:EXT:mc_googlesitemap/Resources/Private/Language/locallang.xlf:tx_mc_googlesitemap_flexform_url
                            </label>
                            <config>
                                <type>text</type>
                                <cols>30</cols>
                                <rows>6</rows>
                            </config>
                        </TCEforms>
                    </settings.url>
                    <settings.changfreq>
                        <TCEforms>
                            <label>Change Frequency (Optional)</label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items type="array">
                                    <numIndex index="0">
                                        <numIndex index="0"></numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                    <numIndex index="1">
                                        <numIndex index="0">Always</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                    <numIndex index="2">
                                        <numIndex index="0">Hourly</numIndex>
                                        <numIndex index="1">2</numIndex>
                                    </numIndex>
                                    <numIndex index="3">
                                        <numIndex index="0">Daily</numIndex>
                                        <numIndex index="1">3</numIndex>
                                    </numIndex>
                                    <numIndex index="4">
                                        <numIndex index="0">Weekly</numIndex>
                                        <numIndex index="1">4</numIndex>
                                    </numIndex>
                                    <numIndex index="5">
                                        <numIndex index="0">Monthly</numIndex>
                                        <numIndex index="1">5</numIndex>
                                    </numIndex>
                                    <numIndex index="6">
                                        <numIndex index="0">Yearly</numIndex>
                                        <numIndex index="1">6</numIndex>
                                    </numIndex>
                                    <numIndex index="7">
                                        <numIndex index="0">Never</numIndex>
                                        <numIndex index="1">7</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.changfreq>
                    <settings.priority>
                        <TCEforms>
                            <label>Priority (Option)</label>
                            <config>
                                <type>input</type>
                                <size>5</size>
                                <range>
                                    <lower>0</lower>
                                    <upper>1</upper>
                                </range>
                                <eval>nospace</eval>
                            </config>
                        </TCEforms>
                    </settings.priority>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>